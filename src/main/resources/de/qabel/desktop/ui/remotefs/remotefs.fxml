<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.qabel.desktop.ui.remotefs.RemoteFSController">
    <children>
        <TreeTableView fx:id="treeTable" prefHeight="550.0" prefWidth="600.0">
            <columns>
                <TreeTableColumn fx:id="nameColumn" prefWidth="200.0" minWidth="50" text="%name"/>
                <TreeTableColumn fx:id="sizeColumn" prefWidth="70.0" text="%size"/>
                <TreeTableColumn fx:id="dateColumn" prefWidth="120.0" text="%date"/>
                <TreeTableColumn fx:id="optionsColumn" prefWidth="150.0" minWidth="100"/>
            </columns>
        </TreeTableView>
    </children>
</VBox>
